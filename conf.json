{
	"repos":"pysrc/gitapi", // 存放资源的仓库名
	"index":{ // 首页显示文章路径（在仓库中的文件路径）
		"path":"mds/hello.md",
	},
	"mostCount": 5, // 侧边栏显示条数，查找不受此限制
	"files":[ // 仓库中的文件列表
		{
			"path": "mds/hello.md", // 路径
			"tags":[ // 标签
				"hello",
				"markdown",
				"demo"
			]
		},
		{
			"path": "mds/second.md",
			"tags":[
				"second",
				"demo"
			]
		}
	]
}
